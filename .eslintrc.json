{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": [
        "html"
    ],
    "extends": [
        "google",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": "off"
    }
}